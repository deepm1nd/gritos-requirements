# .env (in requirements_web_backend)
PORT=3000
# Replace with your actual GitHub App Client ID during development
GITHUB_CLIENT_ID=YOUR_GITHUB_APP_CLIENT_ID
# Replace with your actual GitHub App Client Secret during development
GITHUB_CLIENT_SECRET=YOUR_GITHUB_APP_CLIENT_SECRET
# Replace with your GitHub Personal Access Token (PAT) with 'repo' scope
GITHUB_TOKEN_PAT=YOUR_GITHUB_PERSONAL_ACCESS_TOKEN
# Replace with your GitHub username or organization name
GITHUB_OWNER=your-github-username-or-org
# Replace with your repository name
GITHUB_REPO=product-requirements
# A strong, random secret key for JWT signing
JWT_SECRET=a_very_strong_and_random_secret_key_for_jwt
# Base URL of the frontend application (for redirects, etc.)
WEBUI_BASE_URL=http://localhost:5173
# Path to the SQLite database
DATABASE_PATH=../docs_build/requirements.sqlite
